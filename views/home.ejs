<%- include('partials/header') %>

<section class="left-side-bar">

  <div class="d-flex flex-column flex-shrink-0 p-3 text-white bg-dark" style="width: 280px; height:100vh;">
      <svg class="bi me-2" width="40" height="32"><use xlink:href="#bootstrap"/></svg>
      <span class="fs-4">Asin Analysis</span>

    <hr>
    <ul class="nav nav-pills flex-column mb-auto">
      <li class="nav-item">
        <!-- <a href="#" class="nav-link active" aria-current="page"> -->
          <svg class="bi me-2" width="16" height="16"><use xlink:href="#home"/></svg>
          <div class="input-group">
          <form action="/" method="post">
            <input id="asinInput" type="text" name="asinIdInput" value="" placeholder="Type Asin">
            <button class="btn btn-primary" type="submit">Go</button>
          </form>
          </div>
        <!-- </a> -->
      </li>
      <li>
        <a href="#" class="nav-link text-white">
          <svg class="bi me-2" width="16" height="16"><use xlink:href="#speedometer2"/></svg>
          Dashboard
        </a>
      </li>
      <li>
        <a href="#" class="nav-link text-white">
          <svg class="bi me-2" width="16" height="16"><use xlink:href="#table"/></svg>
          Orders
        </a>
      </li>
      <li>
        <a href="#" class="nav-link text-white">
          <svg class="bi me-2" width="16" height="16"><use xlink:href="#grid"/></svg>
          Products
        </a>
      </li>
      <li>
        <a href="#" class="nav-link text-white">
          <svg class="bi me-2" width="16" height="16"><use xlink:href="#people-circle"/></svg>
          Customers
        </a>
      </li>
    </ul>
    <hr>
  </div>

</section>

<section class="main-table">
<table class="table table-hover">
<thead>
<tr>
  <th scope="col">#</th>
  <th scope="col">Date</th>
  <th scope="col">ASIN</th>
  <th scope="col">Price</th>
  <th scope="col">Category</th>
  <th scope="col">SubCategory</th>
  <th scope="col">Fees</th>
</thead>
<tbody class="body-parameters">
<tr>
  <th scope="row">1</th>
  <td><%=date%></td>
  <td><%=asinNumber%></td>
  <td><%=priceNumber%></td>
  <td><%=categoryNumber%></td>
  <td><%=subCategoryNumber%></td>
  <td><%=fbaFeeNumber%></td>
</tr>
</tbody>
</table>
</section>
<!-- JavaScript Bundle with Popper -->
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

